
<%- include('header'); -%>
    <div class="box" id="heading">
        <h1><%= listTitle %> !</h1>
    </div>

    <div class="box">
        <% for(let i = 0;i<listItems.length;i++){ %> 
          <div class="item">
            <input type="checkbox">
            <p><%= listItems[i]  %> </p>
          </div>
            <%  } %> 
        <form class = "item" action="/" method="POST">
        <input type="text" name="newItem" placeholder="add task" autocomplete="off" autofocus="true">
        <button type="submit" name="list" value=<%= listTitle  %> >+</button>
    </form>
    </div>
    <%- include('footer'); -%>
    
